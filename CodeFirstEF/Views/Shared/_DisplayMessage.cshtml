@using CodeFirstEF.Utils;
@{
    var serviceResult = (ServiceResult)TempData["Service_Result"] ?? null;
}

@if (serviceResult != null)
{
    @ViewHelpers.Alert(serviceResult.Message, serviceResult.Success)
}
<script type="text/javascript">
    $(function () {
        $('.displaymessage').fadeOut(3000);
    })
</script>
