@using StackExchange.Profiling
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/login")
    @Styles.Render("~/Content/kendo/2012.3.1114/css")
    @Scripts.Render("~/bundles/modernizr")
    <!--This bundle was moved by the Kendo UI VS Extensions for compatibility reasons-->
    <script src="@Url.Content("~/Scripts/kendo/2012.3.1114/jquery.min.js")"></script>
    <script src="@Url.Content("~/Scripts/kendo/2012.3.1114/kendo.all.min.js")"></script>
    <script src="@Url.Content("~/Scripts/kendo/2012.3.1114/kendo.aspnetmvc.min.js")"></script>
    @Scripts.Render("~/bundles/jqueryval")

</head>
<body>
    <div class="container">
        <div class="header" class="w800">
            <a href="http://dichan.sina.com.cn/" target="_blank" class="leftlogo" title="新浪地产网"><em>新浪地产</em></a>
            <div class="fr">
                <a href="http://www.sina.com.cn/" target="_blank" class="rightlogo mt5 mr10" title="新浪">新浪首页</a>
                <span class="cut fr mt5"></span>
                <a href="http://dichan.sina.com.cn/" target="_blank" class="fr mt5 mr10" title="新浪地产网">新浪地产网首页</a>
                <div class="clearfix"></div>
                <div class="mt10 c_gray fr">如遇注册问题请拨打 400-606-6969  提示拨号请依次选择“0”、 “7”。</div>
            </div>
        </div>
        <div class="clearfix">
        </div>
        <!--header cut-->
        <div class="logincontent">
            <div class="loginpart">
                <div class="main">
                    <div class="sub">
                        <h6><span>登录新浪地产网</span></h6>
                    </div>
                    <div class="login_form min_h">
                        @RenderBody()
                    </div>
                </div>
            </div>
            <div class="clearfix">
            </div>
        </div>
        <div class="clearfix">
        </div>
    </div>
    @RenderSection("scripts", required: false)

    @MiniProfiler.RenderIncludes(position: RenderPosition.Left, showTrivial: false, showTimeWithChildren: false)
</body>
</html>
